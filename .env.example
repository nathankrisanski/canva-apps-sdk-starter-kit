# REQUIRED: The Agency Middleware API Credentials
# Get these from your Agency Middleware dashboard
# The app will not work without these credentials
AGENCY_CLIENT_ID=your_client_id_here
AGENCY_CLIENT_SECRET=your_client_secret_here

# REQUIRED: Canva Backend Host for webhook/API communication
CANVA_BACKEND_HOST=https://your-backend-url.com

# Optional: App origin for Hot Module Replacement during development
# CANVA_APP_ORIGIN=https://app-your-app-id.canva-apps.com

# Optional: Enable Hot Module Replacement for development
# CANVA_HMR_ENABLED=true